{
    "manifest_version": 2,

    "name": "go-ogle",
    "description": "search google and stuff",
    "version":  "0.1",

    "permissions": [
        "activeTab",
        "tabs",
        "https://www.google.com/search*"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_title": "go-ogle"
    }
}